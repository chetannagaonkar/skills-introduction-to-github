                                  // Prevent copying of grapper tag
                                  let Grapper = document.getElementsByClassName('open_grepper_editor');

                                  if(Grapper){
                                    for(var i = 0; i < Grapper.length; i++)
                                      {
                                        Grapper[i].remove();
                                      }
                                  }

